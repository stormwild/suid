{
  "name": "@suid/codemod",
  "version": "0.9.4",
  "description": "Tool for migrating MUI React code to SUID SolidJS.",
  "keywords": [
    "solid",
    "solidjs",
    "react",
    "components",
    "mui",
    "material-ui",
    "material design",
    "codemod"
  ],
  "type": "module",
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "clean": "tsc --build --clean tsconfig.build.json",
    "watch": "tsc --build -w tsconfig.build.json"
  },
  "dependencies": {
    "@types/micromatch": "^4.0.2",
    "commander": "^9.5.0",
    "fast-glob": "^3.2.12",
    "micromatch": "^4.0.5",
    "prettier": "^2.8.7",
    "ts-morph": "^17.0.1"
  },
  "devDependencies": {
    "typescript": "^5.0.3"
  },
  "publishConfig": {
    "directory": "lib"
  },
  "x-wspa": {
    "pkgManifest": {
      "bin": "bin.js"
    }
  }
}
